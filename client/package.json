{
  "name": "base-client",
  "version": "0.0.1",
  "description": "Base project client",
  "main": "index.js",
  "scripts": {
    "dev": "webpack -d --devtool='source-map' --watch",
    "build": "webpack -p --aiv-log-none",
    "test": "MESSAGE_LEVEL='debug' mocha --require ignore-styles --compilers js:babel-register",
    "mocha": "mocha --require ignore-styles --compilers js:babel-register",
    "cover": "nyc _mocha --require ignore-styles --compilers js:babel-register ",
    "build:langs": "babel scripts/translate.js | node"
  },
  "author": "Eric L. Wittle",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-immutable": "^3.1.0",
    "redux-thunk": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:/Wittle-South-LLC/base.git"
  },
  "nyc": {
    "exclude": [
      "**/test-*.js"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [
      ".jsx"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "expect": "^1.20.2",
    "file-loader": "^0.10.0",
    "glob": "^7.1.1",
    "html-webpack-inline-chunk-plugin": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "immutablediff": "^0.4.4",
    "intl-messageformat-parser": "^1.2.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nock": "^9.0.6",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-auto-inject-version": "^0.2.11",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.3.0"
  }
}
